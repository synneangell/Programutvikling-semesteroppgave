<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootEndreArrangement" prefHeight="728.0" prefWidth="754.0" styleClass="root" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.controller.EndreArrangementController">
    <children>
        <Label layoutX="21.0" layoutY="86.0" prefHeight="20.0" prefWidth="450.0" styleClass="underoverskrift" stylesheets="@styles.css" text="Trykk på element for å gjøre endringer på et arrangement:" />
        <Button fx:id="btnTilbake" layoutX="8.0" layoutY="14.0" mnemonicParsing="false" onAction="#Tilbake" prefHeight="27.0" prefWidth="83.0" styleClass="andreKnapper" text="Tilbake" />
        <Button fx:id="btnSlettArrangement" layoutX="508.0" layoutY="553.0" mnemonicParsing="false" onAction="#slettArrangement" prefHeight="27.0" prefWidth="217.0" styleClass="andreKnapper" text="Slett valgt arrangement" />
        <TableView fx:id="tableviewArrangementer" layoutX="13.0" layoutY="120.0" prefHeight="401.0" prefWidth="728.0">
            <columns>
                <TableColumn fx:id="TypeColumn" prefWidth="75.0" text="Type" />
                <TableColumn fx:id="ArrangementNavnColumn" onEditCommit="#endreNavnArrangement" prefWidth="313.0" text="Arrangementnavn" />
                <TableColumn fx:id="KlokkeslettColumn" onEditCommit="#endreKlokkeslettArrangement" prefWidth="211.0" text="Klokkeslett" />
                <TableColumn fx:id="DatoColumn" onEditCommit="#endreDatoArrangement" prefWidth="125.0" text="Dato" />
            </columns>
        </TableView>
        <Label layoutX="246.0" layoutY="31.0" prefHeight="26.0" prefWidth="318.0" styleClass="overskrift" stylesheets="@styles.css" text="Gjør endringer/slett arrangement" />
      <Button fx:id="btnAvslutt1" layoutX="609.0" layoutY="601.0" mnemonicParsing="false" onAction="#Avslutt" prefHeight="27.0" prefWidth="116.0" styleClass="andreKnapper" text="Avslutt" />
      <ComboBox fx:id="lagreTilFilBox" layoutX="616.0" layoutY="31.0" prefHeight="27.0" prefWidth="123.0" />
      <Label layoutX="616.0" layoutY="14.0" text="Lagre endringer:" />
      <Button fx:id="btnLagre" layoutX="620.0" layoutY="69.0" mnemonicParsing="false" onAction="#Lagre" prefHeight="27.0" prefWidth="116.0" styleClass="andreKnapper" text="Lagre" />
        <TextField fx:id="txtSøk" layoutX="13.0" layoutY="561.0" onKeyTyped="#SøkeHistorie" prefHeight="38.0" prefWidth="217.0" promptText="Søk..." />
        <Label layoutX="14.0" layoutY="543.0" text="For å filtrere arrangementer:" />
    </children>
</AnchorPane>
