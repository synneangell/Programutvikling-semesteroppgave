        <Label layoutX="21.0" layoutY="86.0" prefHeight="20.0" prefWidth="450.0" styleClass="underoverskrift" stylesheets="@styles.css" text="Trykk på element for å gjøre endringer på et arrangement:" />
        <Button fx:id="btnTilbake" layoutX="8.0" layoutY="14.0" mnemonicParsing="false" onAction="#Tilbake" prefHeight="27.0" prefWidth="83.0" styleClass="andreKnapper" text="Tilbake" />
