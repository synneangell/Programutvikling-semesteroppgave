      <Button fx:id="btnAvslutt1" layoutX="609.0" layoutY="601.0" mnemonicParsing="false" onAction="#Avslutt" prefHeight="27.0" prefWidth="116.0" styleClass="andreKnapper" text="Avslutt" />
    </children>
</AnchorPane>
